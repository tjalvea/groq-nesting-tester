    "@sanity/webhook": "^1.0.2",
    "groq": "^2.15.0",
    "lerna": "^4.0.0",
    "next": "^12.1.0",
    "next-sanity": "^0.1.7",
    "next-themes": "^0.1.1",
    "nightwind": "^1.1.12",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-json-pretty": "^2.2.0",
    "slugify": "^1.6.5"
